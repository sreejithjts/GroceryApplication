<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

<!--for report gen when the test cases are executed-->
<listeners>
 <listener class-name="reportGeneration.Listener">
 </listener>
 </listeners>
 
  <test thread-count="5" name="Test">
  <!--first time running browser = chrome-->
  <parameter name="browser" value="chrome"></parameter>
    <classes>
    <!-- short cut to comment cntl+shift+/-->
    <!--<class name="testscript.HomeTest"/>-->
    <!--<class name="testscript.ManageNewsTest"/>-->
    <!--<class name="testscript.AdminTest"/>-->
    <!--<class name="testscript.LoginTest"/>-->
    <!--this is to run a single method/test present in the logintest class-->
    <class name = "testscript.LoginTest">
      <methods>
      <include name="verifyLoginWithInvalidUsernameInvalidPassword"></include>
      <!--to exclude one test method to run in testNG,<exclude name=""></exclude>-->
      </methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
